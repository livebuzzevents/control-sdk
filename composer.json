{
  "name": "livebuzz/control-sdk",
  "description": "LiveBuzz Control SDK",
  "keywords": [
    "livebuzz",
    "control",
    "sdk"
  ],
  "license": "proprietary",
  "type": "package",
  "require": {
    "php": "^8.3",
    "livebuzz/essentials-sdk": "4.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~11.0",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.4.4",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Buzz\\Control\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
