{
  "name": "livebuzz/control-sdk",
  "description": "LiveBuzz Control SDK",
  "keywords": [
    "livebuzz",
    "control",
    "sdk"
  ],
  "license": "proprietary",
  "type": "package",
  "require": {
    "php": "^7.1",
    "jtdsoft/essentials-sdk": "v1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "squizlabs/php_codesniffer": "3.*",
    "mockery/mockery": "*",
    "fzaninotto/faker": "1.4.0",
    "theseer/phpdox": "*"
  },
  "autoload": {
    "psr-4": {
      "Buzz\\Control\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
